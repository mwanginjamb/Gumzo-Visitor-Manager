{
  "name": "visitor-sync-service",
  "version": "1.0.0",
  "description": "Backend synchronization service for visitor management system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:create": "sequelize-cli migration:create --name",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mariadb": "^3.2.2",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
